{
  "name": "rhms-api",
  "version": "0.2.9",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --ext ts --exec ts-node src/bin/www.ts",
    "build": "tsc",
    "start": "node dist/bin/www.js",
    "test": "jest",
    "migrate": "sequelize db:migrate",
    "migrate:run": "sequelize db:migrate --name",
    "migrate:create": "sequelize migration:create --name ",
    "migrate:undo": "sequelize db:migrate:undo --name",
    "migrate:undo:last": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:create": "npx sequelize-cli seed:generate --name",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "seed:run": "npx sequelize-cli db:seed --seed "
  },
